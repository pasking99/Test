ntById(\"notlog\").style.display = \"none\";\r\n      document.getElementById(\"remote\").style.display = \"none\";\r\n      setTimeout(entwarn3, 2000);\r\n      setTimeout(entwarn2, 5000);\r\n      setTimeout(entwarn1, 10000);\r\n    } \r\n  }\r\n} \r\nfunction ven() {\r\n  var x = document.getElementById(\"gruen\");\r\n  var y = document.getElementById(\"rot\");\r\n  x.style.display = \"block\";\r\n  y.style.display = \"none\";\r\n  document.getElementById(\"ventil\").firstChild.data = \"Ventil schließen\";\r\n} \r\nfunction logout() {\r\n  document.getElementById(\"notlog\").style.display = \"block\";\r\n  document.getElementById(\"remote\").style.display = \"block\";\r\n  const music = new Audio('twist.mp3');\r\n  music.play();\r\n} \r\nfunction warn1() {\r\n  document.getElementById(\"w1\").style.display = \"block\";\r\n  document.getElementById(\"wg1\").style.display = \"block\";\r\n} \r\nfunction warn2() {\r\n  document.getElementById(\"w2\").style.display = \"block\";\r\n  document.getElementById(\"wg2\").style.display = \"block\";\r\n} \r\nfunction warn3() {\r\n  document.getElementById(\"w3\").style.display = \"block\";\r\n  document.getElementById(\"wg3\").style.display = \"block\";\r\n} \r\nfunction entwarn1() {\r\n  document.getElementById(\"w1\").style.display = \"none\";\r\n  document.getElementById(\"wg1\").style.display = \"none\";\r\n} \r\nfunction entwarn2() {\r\n  document.getElementById(\"w2\").style.display = \"none\";\r\n  document.getElementById(\"wg2\").style.display = \"none\";\r\n} \r\nfunction entwarn3() {\r\n  document.getElementById(\"w3\").style.display = \"none\";\r\n  document.getElementById(\"wg3\").style.display = \"none\";\r\n  const music = new Audio('Ende.mp3');\r\n  music.play();\r\n} \r\nfunction zehn() {\r\n  if (first === 1)\r\n  {\r\n    const music = new Audio('10.mp3');\r\n    music.play();\r\n  }\r\n}\r\nfunction zwanzig() {\r\n  if (first === 1)\r\n  {\r\n    const music = new Audio('20.mp3');\r\n    music.play();\r\n  